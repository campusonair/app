{
  "name": "iemeshi.jp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.8.1",
    "@wordpress/i18n": "^3.12.0",
    "bootstrap": "^4.4.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-icon": "^1.0.0",
    "react-icons": "^3.9.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "start": ". ./.env && react-scripts start",
    "build": ". ./.env && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "makepot": "node_modules/@babel/cli/bin/babel.js ./src -x '.tsx' -x '.ts' --config-file ./.babelrc.i18n",
    "msgmerge": "msgmerge -U src/lang/ja.po src/lang/lang.pot --lang=ja",
    "po2json": "po2json src/lang/ja.po src/lang/ja.json -f jed && prettier ./src/lang/ja.json --write",
    "i18n": "npm run makepot && npm run msgmerge"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.4",
    "@wordpress/babel-plugin-makepot": "^3.5.0",
    "node-sass": "^4.14.1",
    "typescript": "^3.7.5"
  }
}
